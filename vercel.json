{
  "version": 2,
  "builds": [
    {
      "config": {
        "install-command": "npm install --force"
      },
      "src": "package.json",
      "use": "@vercel/node"
    
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"]
    }
  ]
}